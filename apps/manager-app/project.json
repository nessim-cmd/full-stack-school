{
  "name": "manager-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/manager-app",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/manager-app && NODE_ENV=production npx next build",
        "cwd": "{workspaceRoot}"
      },
      "outputs": ["{projectRoot}/.next"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/manager-app && npx next dev -p 4004",
        "cwd": "{workspaceRoot}"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/manager-app && npx next start -p 4004",
        "cwd": "{workspaceRoot}"
      }
    }
  }
}
